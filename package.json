{
  "name": "@kylemit/hacker-rank-challenges",
  "version": "1.0.0",
  "description": "Hacker Rank Sample Problems",
  "scripts": {
    "test": "mocha --no-deprecation --require chai/register-expect --require ts-node/register src/**/*.ts --exclude src/**/*.slow.ts",
    "test:all": "mocha --no-deprecation --require chai/register-expect --require ts-node/register src/**/*.ts",
    "test:slow": "mocha --no-deprecation --require chai/register-expect --require ts-node/register src/**/*.slow.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KyleMit/hacker-rank-challenges.git"
  },
  "keywords": ["algorithms","leetcode","hackerrank","data-structures","leetcode-solutions","hackerrank-solutions"],
  "author": "Kyle Mit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KyleMit/hacker-rank-challenges/issues"
  },
  "homepage": "https://github.com/KyleMit/hacker-rank-challenges#readme",
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.2",
    "@types/sinon": "^10.0.11",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "sinon": "^14.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
